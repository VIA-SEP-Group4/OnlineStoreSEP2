<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.AddProduct.AddProductViewController">
   <children>
      <MenuBar layoutY="2.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <VBox layoutY="26.0" prefHeight="373.0" prefWidth="600.0">
         <children>
            <TableView fx:id="tableColumns" prefHeight="330.0" prefWidth="600.0">
              <columns>
                <TableColumn fx:id="nameColumn" editable="false" prefWidth="99.20001220703125" text="Name" />
                <TableColumn fx:id="typeColumn" editable="false" prefWidth="122.4000244140625" text="Type" />
                  <TableColumn fx:id="priceColumn" editable="false" prefWidth="88.79998779296875" text="Price" />
                  <TableColumn fx:id="quantityColumn" editable="false" prefWidth="92.79998779296875" text="Quantity" />
                  <TableColumn fx:id="descriptionColumn" editable="false" prefWidth="195.20001220703125" text="Description" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#onAddProdButton" text="Add ">
                     <HBox.margin>
                        <Insets bottom="20.0" left="65.0" right="20.0" top="20.0" />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                     </padding>
                  </Button>
                  <Button fx:id="editButton" mnemonicParsing="false" onAction="#onEditProdButton" text="Edit">
                     <padding>
                        <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets bottom="20.0" left="65.0" right="20.0" top="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#onDeleteProdButton" text="Delete">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets bottom="20.0" left="65.0" right="20.0" top="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#onBackProdButton" text="Back">
                     <padding>
                        <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets bottom="20.0" left="65.0" right="20.0" top="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
